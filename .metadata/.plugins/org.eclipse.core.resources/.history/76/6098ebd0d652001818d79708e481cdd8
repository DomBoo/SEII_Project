package GUI;

import javafx.scene.Scene;
import javafx.scene.control.Label;
import javafx.scene.layout.StackPane;
import javafx.scene.layout.VBox;
import javafx.stage.Modality;
import javafx.stage.Stage;

public class KeyListFrame {
	public KeyListFrame(Stage pStage) {
		Label lab = new Label("Your PublicKey ... -> 12345");
		
		VBox frame = new VBox();
		frame.getChildren().add(lab);
		
		Scene scene = new Scene(frame,600,400);
		
		Stage newWindow = new Stage();
		newWindow.setTitle("KeyGen");
		newWindow.setScene(scene);
		
		newWindow.setX(pStage.getX()+100);
		newWindow.setY(pStage.getY()+100);
	
		newWindow.initModality(Modality.APPLICATION_MODAL);
		newWindow.show();
	}
}
